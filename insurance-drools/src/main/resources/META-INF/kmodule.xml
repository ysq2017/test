<?xml version="1.0" encoding="UTF-8"?>
<kmodule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://www.drools.org/xsd/kmodule">

    <kbase name="calcXLSKB" packages="rules">
        <!--会把packages下的所有规则加进来，所以不同类别的规则最好放在不同的packages下-->
        <ksession name="ksession-calcXLS"/>
    </kbase>

    <kbase name="insuranceXLSKB" packages="insuranceRules">
        <ksession name="ks-insuranceXLS"/>
    </kbase>

    <kbase name="selfRecommend" packages="selfInsuranceRecommendRules">
        <ksession name="ks-selfRecommend"/>
    </kbase>

    <kbase name="otherRecommend" packages="otherInsuranceRecommendRules">
        <ksession name="ks-otherRecommend"/>
    </kbase>


</kmodule>